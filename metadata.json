{
    "author": "Eric B",
    "source": "https://github.com/Ramorous/puppet-avamar",
    "summary": "Allows configuration of Avamar on target host",
    "license": "Apache-2.0",
    "project_page": "https://github.com/Ramorous/puppet-avamar",
    "issues_url": "https://github.com/Ramorous/puppet-avamar/issues",
    "name": "Ramorous-avamar",
